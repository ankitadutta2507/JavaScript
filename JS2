console.log(Math.floor(Math.random()*100)); //random for a random number, flooor for rounding
console.log(Math.ceil(43.8)); // integr greater or equal
console.log(Number.isInteger(2017));

let changeMe=true;
changeMe=false; //changing
console.log(changeMe);

let myName='Ankita';
let myCity="Kalna";
console.log(`My name is ${myName}. My favorite city is ${myCity}.`);  //String Interpolation

const unknown1 = 'foo';
console.log(typeof unknown1); // Output: string

const unknown2 = 10;
console.log(typeof unknown2); // Output: number

const unknown3 = true; 
console.log(typeof unknown3); // Output: boolean

//Reassignment
let newVariable = 'Playing around with typeof.';
console.log(typeof newVariable);
newVariable=1;
console.log(typeof newVariable);

